<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="model_x_banking_account" model="ir.model">
            <field name="name">Bank Account</field>
            <field name="model">x_banking.account</field>
        </record>
        <record id="model_x_banking_loan" model="ir.model">
            <field name="name">Banking Loan</field>
            <field name="model">x_banking.loan</field>
        </record>
        <record id="model_x_banking_loan_application" model="ir.model">
            <field name="name">Banking Loan Application</field>
            <field name="model">x_banking.loan.application</field>
        </record>

        <record id="seq_banking_loan_application" model="ir.sequence">
            <field name="name">Loan Application Sequence</field>
            <field name="code">x_banking.loan.application</field>
            <field name="prefix">LOANAPP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
